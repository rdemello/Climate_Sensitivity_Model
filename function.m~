function [outputArg1,] = tempCalc(i,j,oldoldT_u,oldT_d)
    %FUNCTION Summary of this function goes here
    %   Detailed explanation goes here
    
    upper_energy = timestep * (F_selected(i,j) - (a*(oldT_u(i,j))) - (g*(oldT_u(i,j) - oldT_d(i,j))));  %solved from equation
    newT_u(i+1,j) = oldT_u(i,j) + upper_energy/C_u; 

    deep_energy = timestep * g*(oldT_u(i,j) - oldT_d(i,j));
    oT_d(i+1,j) = oldT_d(i,j) + deep_energy/C_d;
    
    outputArg1 = inputArg1;
    outputArg2 = inputArg2;
end

